      "credentials": {
        "httpHeaderAuth": {
          "id": "Ku7I5ahwnWdwj5SM",
          "name": "Supabase Header Auth"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "const items = $input.all();\nconst newItems = [];\n\nfor (const item of items) {\n  const source = item.json;\n  let pages = [];\n  try {\n    const parsed = JSON.parse(source.content);\n    if (parsed.pages && Array.isArray(parsed.pages)) {\n        pages = parsed.pages;\n    } else if (Array.isArray(parsed)) {\n        pages = parsed;\n    } else {\n        pages = [{ page: 1, content: source.content }];\n    }\n  } catch (e) {\n    // Fallback for old content (plain text)\n    pages = [{ page: 1, content: source.content }];\n  }\n  \n  if (pages.length === 0) pages = [{ page: 1, content: source.content }];\n\n  for (const page of pages) {\n    newItems.push({\n      json: {\n        ...source,\n        chunk_content: page.content,\n        page_number: page.page\n      }\n    });\n  }\n}\n\nreturn newItems;"
      },
      "name": "Split Pages",
      "type": "n8n-nodes-base.code",
      "typeVersion": 1,
      "position": [
        620,
        0
      ]
    },
    {
